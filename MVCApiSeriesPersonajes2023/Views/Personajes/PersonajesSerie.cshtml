@model List<Personaje>

<h1>Personajes Serie</h1>

<table class="table table-dark">
    <thead>
        <tr>
            <th>Personaje</th>
            <th>Imagen</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Personaje person in Model)
        {
            <tr>
                <td>
                    <img src="@person.Imagen"
                         style="width: 100px; height: 100px;"/>
                </td>
                <td class="card-body">
                     <h5 class="card-title">@person.Nombre</h5>                   
                </td>
            </tr>

        }
    </tbody>
</table>